{
  "builds": [
    {
      "src": "client/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "^/portfolio\\.html$", "dest": "/client/portfolio.html" },
    { "src": "^/script\\.js$", "dest": "/client/script.js" },
    { "src": "^/style\\.css$", "dest": "/client/style.css" },
    { "src": "^(.*)$", "dest": "/client/index.html" }
  ]
}
